<ipfixConfig>
	<sensorManager id="99">
		<checkinterval>2</checkinterval>
	</sensorManager>
	<observer id="1">
		<filename>/home/thomas/pcap.dump</filename>
		<pcap_filter>ip</pcap_filter>
		<offlineSpeed>-1</offlineSpeed>
		<next>12</next>
	</observer>

<IDSLoadbalancer id="12">
	<queuecount>3</queuecount>
	<PacketSelector type="HashPacketSelector">
	</PacketSelector>
	<!--<PacketSelector type="IpPacketSelector">
		<DestinationIp queueno="2">192.44.88.190</DestinationIp>
		<SourceIp queueno="1">82.55.74.213</SourceIp>
		<SourceIp queueno="0">69.123.232.54</SourceIp>
	</PacketSelector>-->
	<next>13</next>
	<next>14</next>
	<next>15</next>
</IDSLoadbalancer>

<pcapExporterPipe id="13">
	<logfilebasename>/tmp/pcap_exporter_pipe_1</logfilebasename>
	<snaplen>1512</snaplen>
	<sigkilltimeout>4</sigkilltimeout>
	<command>/usr/sbin/tcpdump -Snr -</command>
	<workingpath>/tmp</workingpath>
	<restartonsignal>true</restartonsignal>
	<!--<restartinterval>7</restartinterval>-->
</pcapExporterPipe>

<pcapExporterPipe id="14">
	<logfilebasename>/tmp/pcap_exporter_pipe_2</logfilebasename>
